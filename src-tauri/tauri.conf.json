{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AI 学院",
  "version": "0.1.0",
  "identifier": "cn.modelbest.aixueyuan",
    "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm web:dev",
    "beforeBuildCommand": "pnpm web:build"
  },
  
  "app": {
    "windows": [
      {
        "title": "AI 学院",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
